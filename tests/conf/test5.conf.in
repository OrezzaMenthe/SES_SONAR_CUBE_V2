user="%USER%";
servers= ({
		hostname = "www.ouistiti.net";
		port = 80;
		keepalivetimeout = 5;
		version="HTTP11";
		static_file = {
			docroot = "%PWD%/tests/htdocs";
			allow = ".html,.htm,.css,.js,.txt,*";
			deny = ".htaccess,.php";
		};
		vhosts = ({
			hostname="test";
			static_file = {
				docroot = "%PWD%/tests/htdocs/vhost";
				allow = ".html,.htm,.css,.js,.txt,*";
				deny = ".htaccess,.php";
			};
		});
	});


