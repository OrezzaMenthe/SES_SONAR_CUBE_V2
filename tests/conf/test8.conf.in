user="%USER%";
servers= ({
		hostname = "www.ouistiti.net";
		addr="127.0.0.1";
		port = 8080;
		keepalivetimeout = 5;
		version="HTTP11";
		auth = {
			type="Basic";
			file="%PWD%/tests/conf/passwd";
		};
		document = {
			docroot = "%PWD%/tests/htdocs";
			allow = ".html,.htm,.css,.js,.txt,*";
			deny = ".htaccess,.php";
		};
	});


