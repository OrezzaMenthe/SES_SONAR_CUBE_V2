user="%USER%";
log-file="%LOGFILE%";
servers= ({
		hostname = "www.ouistiti.net";
		port = 8080;
		keepalivetimeout = 5;
		version="HTTP11";
		document = {
			docroot = "%PWD%/tests/htdocs";
			allow = ".html,.*htm*,.css,.js,.txt,*";
			deny = ".htaccess,.cgi,*.php";
			options = "dirlisting,range,rest";
		};
		websocket = {
			docroot = "/tmp";
			deny = "*";
			allow = "echo";
			options = "direct";
		};
	});
