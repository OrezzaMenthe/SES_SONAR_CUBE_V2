user="%USER%";
servers= ({
		hostname = "ouistiti.local";
		port = 8080;
		keepalivetimeout = 5;
		version="HTTP11";
		auth = {
			user = "%USER%";
			type = "None";
		};
		document = {
			docroot = "%PWD%/tests/htdocs";
			allow = ".html,.*htm*,.css,.js,.txt,*";
			deny = ".htaccess,.cgi,.php";
			options = "dirlisting,range,rest";
		};
	});


