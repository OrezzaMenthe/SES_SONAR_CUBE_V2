user="%USER%";
log-file="/tmp/ouistiti.log";
mimetypes = ({
		ext = ".mp3";
		mime = "audio/mp3";
	},
	{
		ext = ".ogg";
		mime = "audio/ogg";
	},
	{
		ext = ".mp4,.m4v";
		mime = "video/mp4";
	},
	{
		ext = ".avi";
		mime = "video/avi";
	});
servers= ({
		hostname = "www.ouistiti.net";
		port = 8080;
		keepalivetimeout = 5;
		version="HTTP11";
		security = "frame";
		document = {
			docroot = "%PWD%/tests/htdocs";
			allow = ".html,.*htm*,.css,.js,.txt,*";
			deny = ".htaccess,.cgi,*.php";
			options = "dirlisting,range,rest";
		};
		redirect = {
			links = ({
				options = "error,permanently";
				destination = "error_414.html";
				origin = 414;
			},{
				options = "permanently";
				defaultpage = "index.html";
				origin = "*/";
			});
		};
	});


